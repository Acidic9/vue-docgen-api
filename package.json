{
  "name": "vue-docgen-api",
  "version": "2.2.3",
  "description": "Toolbox to extract information from Vue component files for documentation generation purposes.",
  "bugs": {
    "url": "https://github.com/vue-styleguidist/vue-docgen-api/issues"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "babel src/ -d --out-dir dist/ --ignore __tests__,__mocks__",
    "build:watch": "babel src/ --watch -d --out-dir dist/ --ignore __tests__,__mocks__",
    "prepublish": "npm run build",
    "test": "mocha -c src/__tests__ tests src/utils/__tests__ --watch"
  },
  "files": [
    "dist",
    "config.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-styleguidist/vue-docgen-api.git"
  },
  "keywords": [
    "vue",
    "documentation-generation",
    "jsdoc",
    "parse"
  ],
  "author": "Rafael Escala",
  "license": "MIT",
  "homepage": "https://github.com/vue-styleguidist/vue-docgen-api#readme",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "doctrine": "^2.0.0",
    "enhanced-require": "^0.5.0-beta6",
    "hash-sum": "^1.0.2",
    "htmlparser2": "^3.9.2",
    "jsdoc-api": "^3.0.0",
    "lru-cache": "^4.1.0",
    "pug": "^2.0.0-rc.4",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.1",
    "vue": "^2.5.0",
    "vue-template-compiler": "^2.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "mocha": "^3.4.2",
    "vue-class-component": "^6.1.2",
    "vue-property-decorator": "^6.0.0",
    "vuex": "^3.0.1"
  }
}
